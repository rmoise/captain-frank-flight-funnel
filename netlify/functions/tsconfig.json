{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../.netlify/functions-serve",
    "rootDir": ".",
    "lib": ["es2020"],
    "resolveJsonModule": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
